{
    "manifest_version": 3,
    "name": "Altiusrt Card Reason Extracter",
    "version": "1.1.2",
    "description": "Extracts reasons from cards that are hidden in the HTML and displays it in the cards table",
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "matches": ["*://*.altiusrt.com/matches/*", "https://tms.fih.ch/matches/*", "*://*.altiusrt.com/*matches/*"],
            "js": ["content.js"]
        }
    ]
}